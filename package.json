{
  "name": "@darioackermann/saml2-js",
  "version": "4.0.3",
  "description": "SAML 2.0 node helpers",
  "author": "darioackermann",
  "license": "Apache-2.0",
  "main": "index.js",
  "engines": {
    "node": ">=10.x"
  },
  "scripts": {
    "build": "coffee --bare -c -o lib-js lib",
    "test": "cross-env NODE_ENV=test mocha --require coffeescript/register test/*.coffee",
    "test-cov": "cross-env NODE_ENV=test nyc --extension .coffee -r html -r text mocha --require coffeescript/register test/*.coffee",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/darioackermann/saml2-js.git"
  },
  "keywords": [
    "saml",
    "node"
  ],
  "bugs": {
    "url": "https://github.com/darioackermann/saml2-js/issues"
  },
  "devDependencies": {
    "coffeescript": "^1.12.7",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.8.10",
    "async": "^3.2.5",
    "debug": "^4.3.5",
    "underscore": "^1.13.6",
    "xml-crypto": "^6.0.0",
    "xml-encryption": "^3.0.2",
    "xmlbuilder2": "^3.1.1",
    "xpath": "^0.0.34"
  },
  "homepage": "https://github.com/darioackermann/saml2-js#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
