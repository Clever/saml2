{
  "name": "@darioackermann/saml2-js",
  "version": "3.0.3",
  "description": "SAML 2.0 node helpers",
  "author": darioackermann",
  "license": "Apache-2.0",
  "main": "index.js",
  "engines": {
    "node": ">=12.x"
  },
  "scripts": {
    "build": "coffee --bare -c -o lib-js lib",
    "test": "NODE_ENV=test mocha --require coffee-script/register test/*.coffee",
    "test-cov": "NODE_ENV=test nyc --extension .coffee -r html -r text mocha --require coffee-script/register test/*.coffee",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/darioackermann/saml2-js.git"
  },
  "keywords": [
    "saml",
    "node"
  ],
  "bugs": {
    "url": "https://github.com/darioackermann/saml2-js/issues"
  },
  "devDependencies": {
    "coffee-script": "^1.12.0",
    "mocha": "^10.0.0",
    "nyc": "^15.0.0"
  },
  "dependencies": {
    "async": "^3.2.0",
    "debug": "^4.3.0",
    "underscore": "^1.8.0",
    "xml-crypto": "^2.0.0",
    "xml-encryption": "^2.0.0",
    "xml2js": "^0.4.0",
    "xmlbuilder2": "^3.0.2",
    "@xmldom/xmldom": "^0.8.2"
  },
  "homepage": "https://github.com/darioackermann/saml2-js#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
